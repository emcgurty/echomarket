  <%#= render :partial => 'layouts/yes_no', :locals =>
                {:form => "nf",:field=> "displayLenderOrganizationName", :radio_id => "public_display_name",
                :radio_num => "publicdisplayname", :which_class=>"radio", :onchange_call => ""} %>
  <% if onchange_call.blank? %>

    <div class="<%= which_class %>" >
      <div style="float:left;" >
        <label class="radio" for="displayYes<%=  radio_num %>" >Yes:
          <%= radio_button(object, field, "1", {:style=>"vertical-align: middle;width:15px;", :id =>"displayYes" + radio_num } ) %>
        </label>
      </div>
      <div style="float:left;" >
        <label class="radio" for="displayNo<%=  radio_num %>" >&nbsp;&nbsp;No:
          <%= radio_button(object, field, "0", {:style=>"vertical-align: middle;width:15px;", :id =>"displayNo" + radio_num }  ) %>
        </label>
      </div>
    </div>

  <% else %>

    <div class="<%= which_class %>">
      <div style="float:left;" >

        <label class="radio" for="displayYes<%=  radio_num %>" >Yes:
          <%= radio_button(object,field, "1",{ :style=>"vertical-align: middle;width:15px;", :id =>"displayYes" + radio_num , :onchange=>onchange_call }) %>
        </label>
      </div>
      <div style="float:left;" >
        <label class="radio" for="displayNo<%=  radio_num %>" >&nbsp;&nbsp;No:
          <%= radio_button(object, field, "0", {:style=>"vertical-align: middle;width:15px;", :id =>"displayNo" + radio_num , :onchange=>onchange_call }) %></label>
      </div>

    </div>
<% end %>