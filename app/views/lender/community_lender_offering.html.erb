					<%= render :partial => 'layouts/yes_no', :locals => {:object => "lenders",:edit=> true,:field=>"partial_period",:radio_id => "present_partial", :radio_num => "present_partial", :which_class=>"radio", :onchange_call => ""} %>
					<span class="error">&nbsp;</span>
				</div>
			</div></td>
		</tr>
		<tr>
			<td style="vertical-align:top;">
			<div style="margin-left:25px;margin-top:5px;">
