			<% if @lenders.present_during_borrowing_period.abs == 1 %>
			<% display_div =  'inline'%>
			<% end %>
			<% end %>
			<div style="display:<%= display_div %>;background-color:#E1F5A9;;margin-left:15px;margin-top:5px;" id="length_of_presence">
				<div style="margin-left:25px;margin-top:5px;margin-right:10px;">
					<label >During the entire lending period?:</label>
					<%= render :partial => 'layouts/yes_no', :locals => {:object => "lenders",:edit=> true,:field=>"entire_period",:radio_id => "present_entire", :radio_num => "present_entire", :which_class=>"radio", :onchange_call => ""} %>
